{
  "manifest_version": 3,
  "name": "URL Blocker",
  "description": "Block URLs opened in new tabs",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "16.png",
      "48": "48.png",
      "128": "128.png"
    }
  },
  "permissions": ["activeTab", "tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }
}
